{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Attach to Cloudflare Worker",
      "port": 9229,
      "skipFiles": ["<node_internals>/**"],
      "sourceMaps": true,
      "trace": true,
      "restart": true,
      "timeout": 30000,
      "resolveSourceMapLocations": [
        "/medium/backend**",
        "!**/node_modules/**"
      ]
    }
  ]
}